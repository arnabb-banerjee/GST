<body class="login">
    <!-- BEGIN LOGO -->

    <div id="loading" style="display:none;width:100vw; height:100vh; background-color:rgba(0,0,0,0.7); z-index:999; position:fixed; margin:0;top:0;left:0;padding-left:49%; padding-top:22%; ">
        <i class="fa fa-spinner fa-spin" style="font-size:48px;color:white;"></i>
    </div>
    <div class="logo">
        @*<a href="index.html">
            <img src="~/assets/pages/img/logo-big.png" alt="" />
        </a>*@
        <a href="/index.html">
            <img src="/assets/layouts/layout4/img/logo-light-bigbook-background-black.png" alt="logo" class="logo-default">
        </a>
    </div>
    <!-- END LOGO -->
    <!-- BEGIN PROGRESS BAR -->
    <div id="loading" style="width:100vw; height:100vh; background-color:rgba(0,0,0,0.7); z-index:999; position:fixed; margin:0;top:0;padding-left:49%; padding-top:22%; display:none;">
        <i class="fa fa-spinner fa-spin" style="font-size:48px;color:white;"></i>
    </div>
    <!-- END PROGRESS BAR -->
    <!-- BEGIN LOGIN -->
    <div class="content">
        @RenderBody();
    </div>
    <div class="copyright">
        @System.DateTime.Today.Year &copy; @System.Configuration.ConfigurationManager.AppSettings["ApplicationName"].ToString()
        <a target="_blank" href="@System.Configuration.ConfigurationManager.AppSettings["PortalURL"].ToString()">@System.Configuration.ConfigurationManager.AppSettings["AuthorName"].ToString()</a> 
    </div>
